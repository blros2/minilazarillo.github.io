@article{simon_descubren_1995,
	title = {Descubren la que podría ser segunda edición de '{El} {Lazarillo} de {Tormes}'},
	url = {http://elpais.com/diario/1995/12/28/cultura/820105201_850215.html},
	abstract = {Descubren la que podría ser segunda edición de  El Lazarillo de Tormes . Antonia Saavedra y su marido, un matrimonio de Barcarrota (Badajoz), han descubierto, emparedada tras uno de los muros de su vivienda familiar, la que podría ser considerada la segunda edición de El Lazarillo de Tormes. La Junta de Extremadura, que se ha hecho cargo del hallazgo, ha abonado algo más de 15 millones de pesetas por los 11 volúmenes encontrados en su domicilio. Se cree que los libros pudieron ser escondidos allí por alguien que huía de la Inquisición. La primera edición se perdió, después hubo otra de febrero de 1554, y ésta que es de unos días después, de marzo de 1554, fechada en Medina del Campo.},
	language = {es},
	urldate = {2016-04-27},
	journal = {El País},
	author = {Simon, Jeremias Clemente},
	month = dec,
	year = {1995}
}

